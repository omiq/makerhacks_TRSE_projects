program move_to;
var

@include "beeb_constants.ras"

@include "textmode.ras"
	

begin

	// Clear acreen
	cls();							
	
	// Set to 4x colour screen mode, 40x25
	screen_mode(1);					

	// Yellow
	text_colour(2);		

	// Move to 20,20 char location on screen
	move_to(20,20);
	
	// Print with no return			
	print_string("Hello BBC Micro! ", False);		
		
	
end.
